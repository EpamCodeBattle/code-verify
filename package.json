{
  "name": "code-verify",
  "version": "1.0.0",
  "description": "",
  "author": "Front Spot",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test start:backend lint:watch",
    "start:backend": "cross-env NODE_ENV=development nodemon app --exec babel-node",
    "open:src": "cross-env NODE_ENV=development babel-node app",
    "lint": "esw --fix --ignore-path .gitignore",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm run test:server",
    "test:server": "mocha --reporter spec progress tools/testSetup.js \"app/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:babel": "babel app -d dist",
    "prebuild": "npm-run-all clean-dist",
    "build": "npm-run-all build:babel",
    "postbuild": "cross-env NODE_ENV=production node dist"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2017-node7": "^0.5.2",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mocha": "^4.0.1",
    "mongoose": "^4.12.3",
    "nconf": "^0.8.5",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "pre-commit": "^1.2.2",
    "stylelint": "^8.2.0"
  },
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "flow-bin": "^0.57.3"
  }
}
